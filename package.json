{
  "name": "remix-template-remix",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-p build:*",
    "build:remix": "remix build",
    "build:css": "sass styles/_entry.scss app/global.css",
    "predev": "npm run build:css",
    "dev": "run-p dev:*",
    "dev:css": "npm run build:css -- --watch",
    "dev:remix": "cross-env PUBLIC_ENV=local remix dev",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "bootstrap": "^5.1.3",
    "dotenv": "^16.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.52.1",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
